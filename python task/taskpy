

import re


output=[]
def aidojo(n):
    for i in range(1,n+1):
        if i%3==0 and i%5==0:
            output.append('fizzbuzz')
        elif i%3==0 :
            output.append('fizz')
        elif i%5==0 :
            output.append('buzz')
        else:
            output.append(str(i))
    print(output)


Ai=aidojo(3)

dojo=aidojo(5)

Aidojo=aidojo(15)